@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
   @*  Inline using @ *@
    <h1>The Current Time is @DateTime.Now</h1>
    <h1>The Current Year is @DateTime.Now.Year</h1>

   @*  Multiline using @{} *@
    @{
        var name = "Joe Dane";
        var age = 21;
    }

    <h2>Name: @name</h2>
    <h2>Age: @age</h2>
</div>
